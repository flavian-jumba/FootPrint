{
  "version": 3,
  "routes": [
    { 
      "src": "/build/(.*)", 
      "dest": "/build/$1",
      "continue": true
    },
    { 
      "src": "/favicon\\.ico", 
      "dest": "/favicon.ico",
      "continue": true
    },
    {
      "src": "/robots\\.txt",
      "dest": "/robots.txt",
      "continue": true
    },
    {
      "src": "/storage/(.*)",
      "dest": "/storage/$1",
      "continue": true
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1",
      "continue": true
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1",
      "continue": true
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}